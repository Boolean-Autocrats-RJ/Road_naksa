<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.booleanautocrats.road_naksha">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo3456"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo3456"
        android:supportsRtl="true"
        android:theme="@style/Theme.Road_naksha">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".ComplaintActivity"/>
        <activity android:name=".AdminLoginActivity"/>
        <activity android:name=".feedActivity"/>
        <activity android:name=".adimFeedActivity"/>
        <activity android:name=".adminMainActivity"/>
        <activity android:name=".uploadActivity"/>

        <activity android:name=".splashActivity" android:theme="@style/splashTheme" >


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>